<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com/yts8/sixuniverse/roomImage/repository/RoomImageMapper">

    <insert id="save" parameterType="java.util.List">
        insert into room_img (room_id, room_img, img_order) values
        <foreach collection="list" item="roomImageDto" separator=",">
            (#{roomImageDto.roomId}, #{roomImageDto.roomImg}, #{roomImageDto.imgOrder})
        </foreach>
    </insert>

    <select id="findByRoomId" resultType="RoomImageDto">
        select *
        from room_img
        where room_id = #{roomId}
    </select>

</mapper>
